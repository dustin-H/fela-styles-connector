{
  "name": "fela-styles-connector",
  "version": "5.0.0",
  "description": "Allows to connect a fela styles object to a react component",
  "main": "index.js",
  "files": [
    "lib/index.js",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-babel": "NODE_ENV=production babel src -d lib",
    "clean": "rm -rf lib",
    "build": "npm run clean && npm run build-babel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustin-H/fela-styles-connector.git"
  },
  "author": "Dustin Hoffner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dustin-H/fela-styles-connector/issues"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "transform-decorators-legacy"
    ]
  },
  "homepage": "https://github.com/dustin-H/fela-styles-connector#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13"
  }
}
